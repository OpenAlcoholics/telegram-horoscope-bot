{
  "extends": [
    "github>preparingforexams/renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "rate_limit",
        "prep-rate-limiter",
        "ghcr.io/preparingforexams/rate-limiter-migrations"
      ],
      "groupName": "rate_limiter"
